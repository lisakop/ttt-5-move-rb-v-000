#!/usr/bin/env ruby

require_relative '../lib/move.rb'
  board = [" "," "," "," "," "," "," "," "," "]
  print "Welcome to Tic Tac Toe!"
  puts "Where would you like to go?"
  input.to_i = gets.strip
  input_to_index(input)
  move(board, input_to_index, character = "X")
  display_board(board)

